{
  "name": "pocketlib",
  "version": "1.3.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:watch": "ng build --watch",
    "build:prod": "npm run clear && npm run npm-install && npm run ng-build-prod && npm run server-compile && npm run server-copy",
    "clear": "rm -rf dist && rm -rf server/dist",
    "npm-install": "npm install && npm --prefix ./server/ install ./server/",
    "ng-build-prod": "ng build --configuration production --aot",
    "server-compile": "( cd server/ ; tsc )",
    "server-copy": "cpx 'server/{package.json,package-lock.json,server.js,web.config,node_modules/**/*}' dist/ && cpx 'server/dist/**/*' dist/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.8",
    "@angular/cdk": "^12.2.8",
    "@angular/common": "^12.2.8",
    "@angular/compiler": "^12.2.8",
    "@angular/core": "^12.2.8",
    "@angular/forms": "^12.2.8",
    "@angular/localize": "^12.2.8",
    "@angular/material": "^12.2.8",
    "@angular/platform-browser": "^12.2.8",
    "@angular/platform-browser-dynamic": "^12.2.8",
    "@angular/router": "^12.2.8",
    "@angular/service-worker": "^12.2.8",
    "@babel/compat-data": "^7.10.3",
    "@fluentui/web-components": "^1.5.4",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/pro-light-svg-icons": "^5.15.4",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@schematics/angular": "^9.1.15",
    "axios": "^0.21.1",
    "blurhash": "^1.1.3",
    "bootstrap": "^4.5.0",
    "core-js": "^3.16.0",
    "cpx": "^1.5.0",
    "dav-js": "git+https://github.com/dav-apps/dav-js.git#v2.0.8",
    "express": "^4.17.1",
    "http": "0.0.0",
    "jquery": "^3.6.0",
    "jszip": "^3.7.0",
    "localforage": "^1.9.0",
    "ng2-pdf-viewer": "^6.4.1",
    "ngx-file-helpers": "^6.0.0",
    "office-ui-fabric-core": "^11.0.0",
    "pocketlib-server": "file:server",
    "rxjs": "~6.5.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.7",
    "@angular-devkit/core": "^12.2.7",
    "@angular-devkit/schematics": "^12.2.7",
    "@angular/cli": "~12.2.7",
    "@angular/compiler-cli": "~12.2.8",
    "@angular/language-service": "~12.2.8",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jszip": "^3.1.6",
    "@types/node": "^12.11.1",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "codelyzer": "^6.0.0",
