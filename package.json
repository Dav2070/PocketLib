{
	"name": "pocketlib",
	"version": "1.5.4",
	"scripts": {
		"ng": "ng",
		"start": "cd dist && node server.js",
		"build": "npm run build:prod",
		"build:watch": "concurrently 'npm:ng:build:watch' 'npm:server:compile' 'npm:server:copy:watch' 'npm:server:copy2:watch' 'npm:server:watch'",
		"build:prod": "npm run clear && npm run npm:install && npm run ng:build:prod && npm run server:compile && npm run server:copy",
		"build:staging": "npm run clear && npm run npm:install && npm run ng:build:staging && npm run server:compile && npm run server:copy",
		"clear": "rm -rf dist && rm -rf server/dist",
		"npm:install": "npm install && npm --prefix ./server/ install ./server/",
		"ng:build:watch": "ng build --watch",
		"ng:build:prod": "ng build --configuration production --aot",
		"ng:build:staging": "ng build --configuration staging --aot",
		"server:compile": "( cd server/ ; tsc )",
		"server:copy": "cpx 'server/{package.json,package-lock.json,server.js,web.config,node_modules/**/*}' dist/ && cpx 'server/dist/**/*' dist/",
		"server:copy:watch": "cpx 'server/{package.json,package-lock.json,server.js,web.config,.env,node_modules/**/*}' dist/ -w",
		"server:copy2:watch": "cpx 'server/dist/**/*' dist/ -w",
		"server:watch": "cd dist && nodemon server.js",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^15.1.2",
		"@angular/cdk": "^15.1.2",
		"@angular/common": "^15.1.2",
		"@angular/compiler": "^15.1.2",
		"@angular/core": "^15.1.2",
		"@angular/forms": "^15.1.2",
		"@angular/localize": "^15.1.2",
		"@angular/material": "^15.1.2",
		"@angular/platform-browser": "^15.1.2",
		"@angular/platform-browser-dynamic": "^15.1.2",
		"@angular/router": "^15.1.2",
		"@angular/service-worker": "^15.1.2",
		"@babel/compat-data": "^7.10.3",
		"@fortawesome/angular-fontawesome": "^0.12.1",
		"@fortawesome/fontawesome-svg-core": "^6.2.1",
		"@fortawesome/free-brands-svg-icons": "^6.2.1",
		"@fortawesome/free-solid-svg-icons": "^6.2.1",
		"@fortawesome/pro-light-svg-icons": "^6.2.1",
		"@fortawesome/pro-regular-svg-icons": "^6.2.1",
		"@schematics/angular": "^15.1.3",
		"axios": "^0.27.2",
		"blurhash": "^1.1.5",
		"bootstrap": "^5.1.3",
		"core-js": "^3.16.0",
		"cropperjs": "^1.5.12",
		"dav-js": "git+https://github.com/dav-apps/dav-js.git#v2.6.2",
		"dav-ui-components": "file:../../web/dav-ui-components",
		"express": "^4.18.1",
		"http": "0.0.0",
		"jszip": "^3.10.0",
		"localforage": "^1.10.0",
		"ng2-dragula": "^2.1.1",
		"ng2-pdf-viewer": "^6.4.1",
		"ngx-file-helpers": "^9.0.0",
		"office-ui-fabric-core": "^11.0.1",
		"pocketlib-server": "file:server",
		"rxjs": "~6.5.2",
		"tslib": "^2.0.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^15.1.3",
		"@angular-devkit/core": "^15.1.3",
		"@angular-devkit/schematics": "^15.1.3",
		"@angular/cli": "^15.1.3",
		"@angular/compiler-cli": "^15.1.2",
		"@angular/language-service": "^15.1.2",
		"@types/jasmine": "~3.6.0",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "^16.11.11",
		"codelyzer": "^6.0.0",
		"concurrently": "^7.2.1",
		"cpx": "^1.5.0",
		"jasmine-core": "~3.10.1",
		"jasmine-spec-reporter": "~7.0.0",
		"karma": "~6.3.4",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.2",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"nodemon": "^2.0.16",
		"protractor": "~7.0.0",
		"ts-node": "~8.3.0",
		"tslint": "~6.1.0",
		"typescript": "~4.8.4"
	}
}
