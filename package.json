{
  "name": "pocketlib",
  "version": "1.4.3",
  "scripts": {
    "ng": "ng",
    "start": "cd dist && node server.js",
    "build": "npm run build:prod",
    "build:watch": "ng build --watch",
    "build:prod": "npm run clear && npm run npm-install && npm run ng-build-prod && npm run server-compile && npm run server-copy",
    "build:staging": "npm run clear && npm run npm-install && npm run ng-build-staging && npm run server-compile && npm run server-copy",
    "clear": "rm -rf dist && rm -rf server/dist",
    "npm-install": "npm install && npm --prefix ./server/ install ./server/",
    "ng-build-prod": "ng build --configuration production --aot",
    "ng-build-staging": "ng build --configuration staging --aot",
    "server-compile": "( cd server/ ; tsc )",
    "server-copy": "cpx 'server/{package.json,package-lock.json,server.js,web.config,node_modules/**/*}' dist/ && cpx 'server/dist/**/*' dist/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.0.3",
    "@angular/cdk": "^13.0.3",
    "@angular/common": "^13.0.3",
    "@angular/compiler": "^13.0.3",
    "@angular/core": "^13.0.3",
    "@angular/forms": "^13.0.3",
    "@angular/localize": "^13.0.3",
    "@angular/material": "^13.0.3",
    "@angular/platform-browser": "^13.0.3",
    "@angular/platform-browser-dynamic": "^13.0.3",
    "@angular/router": "^13.0.3",
    "@angular/service-worker": "^13.0.3",
    "@babel/compat-data": "^7.10.3",
    "@fluentui/web-components": "^2.0.2",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/pro-light-svg-icons": "^5.15.4",
    "@schematics/angular": "^9.1.15",
    "axios": "^0.24.0",
    "blurhash": "^1.1.4",
    "bootstrap": "^5.1.3",
    "core-js": "^3.16.0",
    "dav-js": "git+https://github.com/dav-apps/dav-js.git#v2.1.1",
    "dav-ui-components": "git+https://github.com/dav-apps/dav-ui-components.git#v0.7.6",
    "express": "^4.17.1",
    "http": "0.0.0",
    "jszip": "^3.7.1",
    "localforage": "^1.10.0",
    "ng2-dragula": "^2.1.1",
    "ng2-pdf-viewer": "^6.4.1",
    "ngx-file-helpers": "^6.0.0",
    "office-ui-fabric-core": "^11.0.0",
    "pocketlib-server": "file:server",
    "rxjs": "~6.5.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.4",
    "@angular-devkit/core": "^13.0.4",
    "@angular-devkit/schematics": "^13.0.4",
    "@angular/cli": "~13.0.4",
    "@angular/compiler-cli": "^13.0.3",
    "@angular/language-service": "^13.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.20.32",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "codelyzer": "^6.0.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.4.4"
  },
  "engines": {
    "node": "14.x",
    "npm": "8.x"
  }
}