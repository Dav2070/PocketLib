{
	"name": "pocketlib",
	"version": "1.7.0",
	"scripts": {
		"ng": "ng",
		"start": "cd dist && node server.js",
		"dev": "concurrently 'npm:ng:build:watch' 'npm:server:compile' 'npm:server:copy:watch' 'npm:server:copy2:watch' 'npm:server:watch'",
		"build": "npm run build:prod",
		"build:prod": "npm run clear && npm run npm:install && npm run ng:build:prod && npm run server:compile && npm run server:copy",
		"build:staging": "npm run clear && npm run npm:install && npm run ng:build:staging && npm run server:compile && npm run server:copy",
		"clear": "rm -rf dist && rm -rf server/dist",
		"npm:install": "npm install --include=dev && npm --include=dev --prefix ./server/ install ./server/",
		"ng:build:watch": "ng build --watch",
		"ng:build:prod": "ng build --configuration production",
		"ng:build:staging": "ng build --configuration staging",
		"server:compile": "( cd server/ ; tsc )",
		"server:copy": "cpx 'server/{package.json,package-lock.json,web.config,node_modules/**/*}' dist/ && cpx 'server/dist/**/*' dist/",
		"server:copy:watch": "cpx 'server/{package.json,package-lock.json,web.config,node_modules/**/*}' dist/ -w",
		"server:copy2:watch": "cpx 'server/dist/**/*' dist/ -w",
		"server:watch": "cd dist && nodemon server.js",
		"lint": "ng lint",
		"serve:ssr:PocketLib": "node dist/PocketLib/server/server.mjs"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^19.0.5",
		"@angular/cdk": "^19.0.4",
		"@angular/common": "^19.0.5",
		"@angular/compiler": "^19.0.5",
		"@angular/core": "^19.0.5",
		"@angular/forms": "^19.0.5",
		"@angular/localize": "^19.0.5",
		"@angular/platform-browser": "^19.0.5",
		"@angular/platform-browser-dynamic": "^19.0.5",
		"@angular/platform-server": "^19.0.5",
		"@angular/router": "^19.0.5",
		"@angular/service-worker": "^19.0.5",
		"@angular/ssr": "^19.0.6",
		"@apollo/client": "^3.12.4",
		"@babel/compat-data": "^7.26.3",
		"@fortawesome/angular-fontawesome": "^1.0.0",
		"@fortawesome/fontawesome-svg-core": "^6.7.2",
		"@fortawesome/free-brands-svg-icons": "^6.7.2",
		"@fortawesome/free-solid-svg-icons": "^6.7.2",
		"@fortawesome/pro-light-svg-icons": "^6.7.2",
		"@fortawesome/pro-regular-svg-icons": "^6.7.2",
		"@tsparticles/confetti": "^3.7.1",
		"apollo-angular": "^8.0.0",
		"axios": "^1.7.9",
		"core-js": "^3.39.0",
		"cropperjs": "^1.6.2",
		"dav-js": "git+https://github.com/dav-apps/dav-js.git#v2.7.3",
		"dav-ui-components": "git+https://github.com/dav-apps/dav-ui-components.git#v1.7",
		"express": "^4.21.2",
		"graphql": "^16.10.0",
		"http": "0.0.0",
		"jszip": "^3.10.1",
		"localforage": "^1.10.0",
		"ng2-pdf-viewer": "^10.4.0",
		"ngx-file-helpers": "^13.0.1",
		"pocketlib-server": "file:server",
		"rxjs": "^7.8.1",
		"tslib": "^2.8.1",
		"zone.js": "^0.15.0"
	},
	"devDependencies": {
		"@angular-devkit/core": "^19.0.6",
		"@angular-devkit/schematics": "^19.0.6",
		"@angular/build": "^19.0.6",
		"@angular/cli": "^19.0.6",
		"@angular/compiler-cli": "^19.0.5",
		"@angular/language-service": "^19.0.5",
		"@types/express": "^5.0.0",
		"@types/node": "^22.10.2",
		"codelyzer": "^6.0.2",
		"concurrently": "^9.1.0",
		"cpx": "^1.5.0",
		"nodemon": "^3.1.9",
		"ts-node": "^10.9.2",
		"tslint": "^6.1.3",
		"typescript": "^5.6.3"
	}
}
