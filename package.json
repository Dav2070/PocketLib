{
  "name": "pocketlib",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:watch": "ng build --watch",
    "build:prod": "npm run clear && npm run npm-install && npm run ng-build-prod && npm run server-compile && npm run server-copy",
    "clear": "rm -rf dist && rm -rf server/dist",
    "npm-install": "npm install && npm --prefix ./server/ install ./server/",
    "ng-build-prod": "ng build --prod --aot",
    "server-compile": "( cd server/ ; tsc )",
    "server-copy": "cpx 'server/{package.json,package-lock.json,server.js,web.config,node_modules/**/*}' dist/ && cpx 'server/dist/**/*' dist/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-react/core": "^1.0.1",
    "@angular-react/fabric": "^1.0.3",
    "@angular/animations": "^8.0.2",
    "@angular/cdk": "^8.0.2",
    "@angular/common": "^8.0.2",
    "@angular/compiler": "^8.0.2",
    "@angular/core": "^8.0.2",
    "@angular/forms": "^8.0.2",
    "@angular/material": "^8.1.2",
    "@angular/platform-browser": "^8.0.2",
    "@angular/platform-browser-dynamic": "^8.0.2",
    "@angular/pwa": "^0.802.1",
    "@angular/router": "^8.0.2",
    "@angular/service-worker": "^8.0.2",
    "@ng-bootstrap/ng-bootstrap": "^5.1.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "core-js": "^3.2.1",
    "cpx": "^1.5.0",
    "dav-npm": "git+https://github.com/dav-apps/dav-npm.git#v1.5",
    "express": "^4.17.1",
    "http": "0.0.0",
    "jquery": "^3.4.0",
    "jszip": "^3.2.2",
    "localforage": "^1.7.3",
    "ng2-pdf-viewer": "^5.3.4",
    "ngx-file-helpers": "^2.0.0",
    "office-ui-fabric-core": "^10.1.0",
    "office-ui-fabric-react": "^6.151.0",
    "pocketlib-server": "file:server",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rxjs": "~6.5.2",
    "socket.io": "^2.2.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.24",
    "@angular/cli": "~8.2.1",
    "@angular/compiler-cli": "~8.2.2",
    "@angular/language-service": "~8.2.2",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jszip": "^3.1.6",
    "@types/node": "~12.7.1",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.0",
